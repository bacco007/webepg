{
  "name": "webepgv2-frontend",
  "version": "0.2.0",
  "private": true,
  "packageManager": "pnpm@10.18.2",
  "scripts": {
    "analyze": "ANALYZE=true pnpm build",
    "clean": "rm -rf .next out",
    "deps:check": "npm-check",
    "deps:unused": "depcheck",
    "deps:unused:deep": "depcheck",
    "deps:updates": "npm-check --skip-unused",
    "deps:audit": "depcheck && npm-check --skip-unused",
    "deps:audit:ci": "depcheck --json || true",
    "dev": "concurrently \"pnpm run dev:frontend\" \"pnpm run dev:backend\"",
    "dev:frontend": "next dev",
    "dev:backend": "cd ../backend && . .venv/bin/activate && uvicorn main:app --reload",
    "build": "next build",
    "start": "next start",
    "preview": "next build && next start",
    "lint:fix": "biome lint --write .",
    "lint": "npx ultracite@latest lint",
    "format": "npx ultracite@latest format",
    "format:fix": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "formatandlint": "biome check --write .",
    "postbuild": "next-sitemap",
    "update": "pnpm dlx npm-check-updates -u && pnpm install",
    "update-shadcn": "pnpm dlx shadcn@latest add --all --overwrite"
  },
  "dependencies": {
    "@drustack/leaflet.resetview": "^1.9.2",
    "@hookform/resolvers": "^5.2.2",
    "@next/bundle-analyzer": "^15.5.5",
