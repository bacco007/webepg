{
  "name": "webepgv2-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "next dev",
    "dev:backend": "cd ../backend && . .venv/bin/activate && uvicorn main:app --reload",
    "build": "next build",
    "start": "next start",
    "preview": "next build && next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --check './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc.js",
    "format:fix": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc.js",
    "formatandlint": "npm run format:fix && npm run lint:fix",
    "postbuild": "next-sitemap",
    "genroutelist": "cd src && npx generate-next-routes-list",
    "update": "npm i -g npm-check-updates && ncu -u && npm i"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.1",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-collapsible": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.1",
    "@radix-ui/react-scroll-area": "^1.2.1",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dayjs": "^1.11.13",
    "lucide-react": "^0.462.0",
    "next": "^15.0.3",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.3",
    "react": "^19.0.0-rc.1",
    "react-dom": "^19.0.0",
    "react-sticky": "^6.0.3",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^19",
    "@types/react-sticky": "^6.0.6",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "autoprefixer": "10.4.20",
    "concurrently": "^9.1.0",
    "eslint": "^9.16.0",
    "eslint-config-next": "^15.0.3",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import-helpers": "2.0.1",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "0.11.1",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "eslint-plugin-testing-library": "7.0.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "globals": "^15.12.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.1",
    "prettier-eslint": "^16.3.0",
    "prettier-plugin-tailwindcss": "0.6.9",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.15",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5",
    "typescript-eslint": "^8.16.0"
  }
}
