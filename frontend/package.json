{
  "name": "webepgv2-frontend",
  "version": "0.2.0",
  "private": true,
  "packageManager": "pnpm@10.18.2",
  "scripts": {
    "analyze": "ANALYZE=true pnpm build",
    "clean": "rm -rf .next out",
    "deps:check": "npm-check",
    "deps:unused": "depcheck",
    "deps:unused:deep": "depcheck",
    "deps:updates": "npm-check --skip-unused",
    "deps:audit": "depcheck && npm-check --skip-unused",
    "deps:audit:ci": "depcheck --json || true",
    "dev": "concurrently \"pnpm run dev:frontend\" \"pnpm run dev:backend\"",
    "dev:frontend": "next dev",
    "dev:backend": "cd ../backend && uv run fastapi dev",
    "build": "next build",
    "start": "next start",
    "preview": "next build && next start",
    "lint:fix": "pnpm biome lint --write .",
    "lint": "pnpm dlx ultracite@latest lint",
    "format": "pnpm dlx ultracite@latest format",
    "format:fix": "pnpm biome format --write .",
    "check": "pnpm biome check .",
    "check:fix": "pnpm biome check --write .",
    "formatandlint": "pnpm biome check --write .",
    "postbuild": "next-sitemap",
    "update": "pnpm up --interactive --latest",
    "update-shadcn": "pnpm dlx shadcn@latest add --all --overwrite",
    "update-biome": "pnpm biome migrate --write"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@drustack/leaflet.resetview": "^1.9.2",
    "@hookform/resolvers": "^5.2.2",
    "@next/bundle-analyzer": "16.1.1",
    "@radix-ui/react-slot": "^1.2.4",
    "@svgr/webpack": "^8.1.0",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dayjs": "^1.11.19",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.26",
    "input-otp": "^1.4.2",
    "leaflet": "^1.9.4",
    "leaflet-draw": "^1.0.4",
    "leaflet.markercluster": "^1.5.3",
    "lucide-react": "^0.562.0",
    "motion": "^12.23.26",
    "next": "16.1.1",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.6",
    "nextjs-toploader": "^3.9.17",
    "nuqs": "^2.8.6",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-day-picker": "9.13.0",
    "react-dom": "19.2.3",
    "react-flagpack": "^2.0.6",
    "react-hook-form": "^7.68.0",
    "react-hotkeys-hook": "^5.2.1",
    "react-leaflet": "^5.0.0",
    "react-resizable-panels": "^3.0.6",
    "react-use-measure": "^2.1.7",
    "recharts": "2.15.4",
    "shadcn": "^3.6.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "vaul": "^1.1.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@commitlint/config-conventional": "^20.3.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/leaflet": "^1.9.21",
    "@types/leaflet-draw": "^1.0.13",
    "@types/node": "^25.0.0",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "concurrently": "^9.2.1",
    "depcheck": "^1.4.7",
    "npm-check": "^6.0.1",
    "react-leaflet-cluster": "^4.0.0",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "ultracite": "6.5.0"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.2.2",
      "@types/react-dom": "19.2.2"
    },
    "onlyBuiltDependencies": [
      "msw"
    ]
  }
}
